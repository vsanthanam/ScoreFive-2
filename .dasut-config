{
    "swiftformat": {
        "enableRules": ["isEmpty"],
        "disableRules": ["blankLinesAtEndOfScope",
                         "blankLinesAtStartOfScope",
                         "redundantNilInit",
                         "unusedArguments",
                         "redundantParens",
                         "wrapMultilineStatementBraces",
                         "trailingCommas",
                         "braces"],
        "excludeDirs": ["ScoreFive/App/Derived",
                        "bin/tuist"],
	"swiftVersion": "5.4"
    },
    "mockolo": {
        "testableImports": ["ShortRibs",
                            "ScoreFive",
			                "Analytics"],
        "destinations": ["ScoreFive/App/ScoreFiveTests/Mocks.swift"]
    },
    "swiftlint": {
        "excludeDirs": ["ScoreFive/App/Derived",
                        "bin",
                        "tooling/sftool/.build",
                        "tooling/dasut/.build"],
        "disabledRules": ["line_length",
                          "nesting",
                          "function_body_length",
                          "identifier_name",
                          "redundant_optional_initialization",
                          "type_body_length",
                          "type_name",
                          "function_parameter_count",
                          "generic_type_name",
                          "unused_setter_value",
                          "trailing_whitespace"],
        "optInRules": []
    },
    "testConfig": {
        "device": "iPhone 8 Plus",
        "os": "14.4"
    },
    "vendorCodePath": "ScoreFive/Vendor",
    "diCodePath": "ScoreFive/App/ScoreFive/Src/Features/",
    "diGraphPath": "ScoreFive/App/ScoreFive/Src/Infra/DependencyGraph.swift",
    "mockPath": "ScoreFive/App/ScoreFiveTests/Mocks.swift",
    "featureCodePath": "ScoreFive/App/ScoreFive/Src",
    "libraryCodePath": "ScoreFive/Libraries",
    "tuist": {
        "generationOptions": ["disableAutogeneratedSchemes",
                              "disableSynthesizedResourceAccessors"],
        "root": "ScoreFive"
    }
}
