{
  "phabricator.uri": "https://phab.vsanthanam.com/",
  "arc.land.onto": [
    "main"
  ],
  "load": [
    "tooling/arctools"
  ],
  "unit.engine": "MultiTestEngine",
  "unit.engine.multi-test.engines": [
    {
      "engine": "CommandUnitTestEngine",
      "unit.engine.command": "./sftool develop --arcunit",
      "unit.engine.command.name": "Check Project Manifest"
    },
    {
      "engine": "CommandUnitTestEngine",
      "unit.engine.command": "sh update-sftool.sh",
      "unit.engine.command.name": "Update SFTool"
    }
  ]
}
